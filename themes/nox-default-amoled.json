                "foreground": "#9cdcfe"
            }
        },
        {
            "scope": "meta.diff.header",
            "settings": {
                "foreground": "#569cd6"
            }
        },
        {
            "scope": "storage",
            "settings": {
                "foreground": "#569cd6"
            }
